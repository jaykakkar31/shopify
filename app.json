{
  "expo": {
		"name": "Shopify",
		"slug": "ShopApp",
		"description": "This is a clone of a online shopping app, implementing features like auth, adding products to cart, ordering as well as adding their own products.",

		"version": "1.0.0",

		"orientation": "portrait",
		"icon": "./assets/adaptive-icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffedff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"bundleIdentifier": "com.jaykakkar31.shopify",
			"buildNumber": "1.0.0",

			"supportsTablet": true
		},
		"android": {
			"package": "com.jaykakkar31.shopify",
			"versionCode": 1,
			"useNextNotificationsApi": true,

			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffedff"
			}
		}

	}
}

	